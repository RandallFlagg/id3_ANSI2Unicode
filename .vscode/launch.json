    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug - .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp2.0/Converter.dll",
            //"args": ["AAA", "-p D:\\Ohad\\mp3 ID3 tag convert\\Data", "-e *.mp3", "BBBB"],
            "args": ["-p D:\\Ohad\\ID3\\mp3 ID3 tag convert\\Data", "-e *.mp3", "-r"],
            //,"-A a", "-B b bb", "C c"],
            "cwd": "${workspaceFolder}",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            
        },
        {
            "name": "Release - .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build_release",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp2.0/Converter.dll",
            //"args": ["AAA", "-p D:\\Ohad\\mp3 ID3 tag convert\\Data", "-e *.mp3", "BBBB"],
            "args": ["-p D:\\Ohad\\ID3\\mp3 ID3 tag convert\\Data", "-e *.mp3", "-r"],
            //,"-A a", "-B b bb", "C c"],
            "cwd": "${workspaceFolder}",
            // For more information about the 'console' field, see https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md#console-terminal-window
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            
        }
